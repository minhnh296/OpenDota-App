<+><?xml version=\"1.0\" encoding=\"utf-8\"?>\n<LinearLayout xmlns:android=\"http://schemas.android.com/apk/res/android\"\n    xmlns:app=\"http://schemas.android.com/apk/res-auto\"\n    xmlns:tools=\"http://schemas.android.com/tools\"\n    android:id=\"@+id/layout1\"\n    android:layout_width=\"match_parent\"\n    android:layout_height=\"match_parent\"\n    tools:context=\".MainActivity\"\n    android:background=\"?attr/backgroundColor\"\n    android:orientation=\"vertical\">\n\n    <RelativeLayout\n        android:id=\"@+id/khunghome_1\"\n        android:layout_width=\"match_parent\"\n        android:layout_height=\"195sp\"\n        android:layout_marginStart=\"5dp\"\n        android:layout_marginTop=\"30dp\"\n        android:layout_marginEnd=\"5dp\"\n        android:background=\"?attr/colorOnSecondary\"\n        tools:ignore=\"MissingConstraints\">\n\n        <TextView\n            android:layout_width=\"wrap_content\"\n            android:layout_height=\"wrap_content\"\n            android:layout_marginTop=\"25dp\"\n            android:layout_marginStart=\"45dp\"\n            android:text=\"OPENDOTA\"\n            android:gravity=\"left\"\n            android:textColor=\"?attr/textFillColor\"\n            android:textSize=\"35sp\">\n        </TextView>\n\n        <ImageView\n            android:layout_width=\"wrap_content\"\n            android:layout_height=\"63dp\"\n            android:layout_alignParentEnd=\"true\"\n            android:layout_marginTop=\"15dp\"\n            android:layout_marginEnd=\"50dp\"\n            android:src=\"@drawable/dota_2\">\n\n        </ImageView>\n\n        <TextView\n            android:id=\"@+id/text1\"\n            android:layout_width=\"match_parent\"\n            android:layout_height=\"wrap_content\"\n            android:layout_centerHorizontal=\"true\"\n            android:layout_marginTop=\"90sp\"\n            android:layout_marginEnd=\"20sp\"\n            android:layout_marginBottom=\"17sp\"\n            android:text=\"Open source Dota 2 data platform\"\n            android:textAlignment=\"center\"\n            android:textColor=\"?attr/textFillColor\"\n            android:textSize=\"25sp\">\n        </TextView>\n\n        <androidx.cardview.widget.CardView\n            android:layout_width=\"match_parent\"\n            android:layout_height=\"wrap_content\"\n            android:layout_marginStart=\"20dp\"\n            android:layout_below=\"@+id/text1\"\n            app:cardBackgroundColor=\"@color/white\"\n            android:layout_marginEnd=\"20dp\">\n\n            <ImageView\n                android:layout_width=\"40dp\"\n                android:layout_height=\"40dp\"\n                android:layout_gravity=\"left\"\n                android:src=\"@drawable/steam_icon\">\n            </ImageView>\n\n            <TextView\n                android:layout_width=\"match_parent\"\n                android:layout_height=\"wrap_content\"\n                android:layout_gravity=\"center\"\n                android:gravity=\"right\"\n                android:text=\" Login for automatic replay parsing \"\n                android:textColor=\"@color/black\"\n                android:textSize=\"20sp\">\n            </TextView>\n        </androidx.cardview.widget.CardView>\n    </RelativeLayout>\n\n    <LinearLayout\n        android:weightSum=\"2\"\n        android:layout_width=\"wrap_content\"\n        android:layout_height=\"220dp\"\n        android:layout_marginTop=\"5dp\"\n        android:layout_marginStart=\"5dp\"\n        android:layout_marginEnd=\"5dp\"\n        android:layout_marginBottom=\"5dp\"\n        android:layout_below=\"@+id/khunghome_1\"\n        android:id=\"@+id/khunghome_2\">\n\n        <LinearLayout\n            android:layout_weight=\"1\"\n            android:layout_width=\"match_parent\"\n            android:layout_height=\"220dp\"\n            android:layout_marginEnd=\"5dp\"\n            android:layout_marginTop=\"10dp\"\n            android:layout_marginBottom=\"288dp\"\n            android:background=\"?attr/colorOnSecondary\"\n            android:gravity=\"center_horizontal\"\n            android:orientation=\"vertical\">\n\n            <ImageView\n                android:id=\"@+id/open_source\"\n                android:layout_width=\"65dp\"\n                android:layout_height=\"67dp\"\n                android:layout_marginTop=\"5dp\"\n                android:src=\"@drawable/open_source_icon\" />\n\n            <TextView\n                android:layout_width=\"wrap_content\"\n                android:layout_height=\"wrap_content\"\n                android:gravity=\"top\"\n                android:text=\"Open Source\"\n                android:textColor=\"#03A9F4\"\n                android:textSize=\"22sp\" />\n\n            <TextView\n                android:layout_width=\"wrap_content\"\n                android:layout_height=\"wrap_content\"\n                android:layout_margin=\"4dp\"\n                android:gravity=\"center_horizontal\"\n                android:maxLines=\"5\"\n                android:text=\"All project code is open source and available for contributors to improve and modify.\"\n                android:textColor=\"?attr/textFillColor\"\n                android:textSize=\"18sp\" />\n        </LinearLayout>\n\n        <LinearLayout\n            android:layout_weight=\"1\"\n            android:layout_width=\"wrap_content\"\n            android:layout_height=\"250dp\"\n            android:layout_marginStart=\"5dp\"\n            android:layout_marginTop=\"10dp\"\n            android:background=\"?attr/colorOnSecondary\"\n            android:gravity=\"center_horizontal\"\n            android:orientation=\"vertical\">\n\n            <ImageView\n                android:layout_width=\"65dp\"\n                android:layout_height=\"75dp\"\n                android:layout_marginTop=\"5dp\"\n                android:src=\"@drawable/stats_icon\" />\n\n            <TextView\n                android:layout_width=\"wrap_content\"\n                android:layout_height=\"wrap_content\"\n                android:gravity=\"top\"\n                android:text=\"In-Depth Data\"\n                android:textColor=\"#03A9F4\"\n                android:textSize=\"22sp\" />\n\n            <TextView\n                android:layout_width=\"wrap_content\"\n                android:layout_height=\"wrap_content\"\n                android:layout_marginTop=\"5dp\"\n                android:gravity=\"center\"\n                android:text=\"Parsing replay files provides highly detailed match data.\"\n                android:textAlignment=\"center\"\n                android:textColor=\"?attr/textFillColor\"\n                android:textSize=\"18sp\" />\n        </LinearLayout>\n\n    </LinearLayout>\n\n\n\n    <LinearLayout\n        android:layout_below=\"@+id/khunghome_2\"\n        android:layout_width=\"match_parent\"\n        android:layout_height=\"200dp\"\n        android:orientation=\"vertical\"\n        android:layout_marginTop=\"10dp\"\n        android:layout_alignParentEnd=\"true\"\n        android:layout_marginStart=\"5dp\"\n        android:layout_marginEnd=\"5dp\"\n        android:background=\"?attr/colorOnSecondary\">\n\n        <ImageView\n            android:id=\"@+id/magic\"\n            android:layout_width=\"65dp\"\n            android:layout_height=\"67dp\"\n            android:layout_gravity=\"center\"\n            android:layout_marginTop=\"15dp\"\n            android:src=\"@drawable/magic_icon\" />\n\n        <TextView\n            android:id=\"@+id/text9\"\n            android:layout_width=\"wrap_content\"\n            android:layout_height=\"wrap_content\"\n            android:layout_gravity=\"center\"\n            android:text=\"Free of Charge\"\n            android:textColor=\"#03A9F4\"\n            android:textSize=\"24sp\" />\n\n        <TextView\n            android:layout_width=\"wrap_content\"\n            android:layout_height=\"wrap_content\"\n            android:layout_below=\"@id/text9\"\n            android:layout_marginTop=\"5dp\"\n            android:layout_marginStart=\"20dp\"\n            android:layout_marginEnd=\"20dp\"\n            android:gravity=\"center\"\n            android:text=\"Server are funded by sponsors and volunteers maintain the code, so the service is offered free of charge.\"\n            android:textColor=\"?attr/textFillColor\"\n            android:textSize=\"18sp\" />\n\n    </LinearLayout>\n\n\n</LinearLayout>
